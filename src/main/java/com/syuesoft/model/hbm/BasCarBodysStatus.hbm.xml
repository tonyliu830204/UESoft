<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.syuesoft.model.BasCarBodysStatus" table="bas_car_bodys_status" >
        <id name="bodyNum" type="java.lang.Short">
            <column name="BODY_NUM" precision="4" scale="0" />
            <generator class="increment" />
        </id>
        <property name="bodyId" type="java.lang.String">
            <column name="BODY_ID" length="2" />
        </property>
        <property name="bodyName" type="java.lang.String">
            <column name="BODY_NAME" length="20"/>
        </property>
        <property name="bodyRemark" type="java.lang.String">
            <column name="BODY_REMARK" length="50" />
        </property>
    </class>
</hibernate-mapping>
